@{
    ViewData["Title"] = "Contact saved!";
}

<div class="text-center">
    <h1>Saved.</h1>

    <a href="/Contact/Index" id="backLink">Back to form</a>
</div>

<script>
    document.addEventListener("keydown", function (event) {
        if (event.key === "Enter") {
            document.getElementById("backLink").click();
        }
    });
</script>
