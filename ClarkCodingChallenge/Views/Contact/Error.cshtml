@model ErrorViewModel;

@{
    ViewData["Title"] = "Error during save operation.";
}

<div class="text-center">
    <h1>An error occurred and the contact was not saved.</h1>

    <p>RequestId: @Model.RequestId</p>

    <a href="/Contact/Index" id="backLink">Back to form</a>
</div>

<script>
    document.addEventListener("keydown", function (event) {
        if (event.key === "Enter") {
            document.getElementById("backLink").click();
        }
    });
</script>
